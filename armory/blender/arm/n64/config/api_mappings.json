{
  "_comment": "Type mappings and filters for N64 code generation",

  "type_maps": {
    "haxe_to_n64": {
      "double": "float",
      "Float": "float",
      "float": "float",
      "Int": "int32_t",
      "int": "int32_t",
      "Bool": "bool",
      "bool": "bool"
    }
  },

  "filters": {
    "skip_type_prefixes": ["iron__", "kha__", "armory__", "hl_", "vdynamic", "varray"],
    "skip_member_names": ["object", "transform", "gamepad", "keyboard", "mouse", "name"],
    "supported_types": ["double", "Float", "float", "Int", "int", "Bool", "bool"]
  }
}
