{
	"contexts": [
		{
			"name": "translucent_resolve",
			"depth_write": true,
			"compare_mode": "always",
			"cull_mode": "none",
			"blend_source": "blend_one",
			"blend_destination": "blend_one",
			"blend_operation": "add",
			"blend_source_alpha": "blend_zero",
			"blend_destination_alpha": "inverse_source_alpha",
			"blend_operation": "add",
			"links": [
				{
					"name": "texSize",
					"link": "_screenSize"
				},
				{
					"name": "P",
					"link": "_projectionMatrix",
					"ifdef": ["_SSRefraction"]
				},
				{
					"name": "V3",
					"link": "_viewMatrix3",
					"ifdef": ["_SSRefraction"]
				},
				{
					"name": "invP",
					"link": "_inverseProjectionMatrix",
					"ifdef": ["_SSRefraction"]
				},
				{
					"name": "cameraProj",
					"link": "_cameraPlaneProj",
					"ifdef": ["_SSRefraction"]
				},
				{
					"name": "PPComp9",
					"link": "_PPComp9",
					"ifdef": ["_CPostprocess"],
					"ifdef": ["_SSRefraction"]
				},
				{
					"name": "PPComp10",
					"link": "_PPComp10",
					"ifdef": ["_CPostprocess"],
					"ifdef": ["_SSRefraction"]
				}
			],
			"vertex_shader": "../include/pass_viewray2.vert.glsl",
			"fragment_shader": "translucent_resolve.frag.glsl"
		}
	]
}
